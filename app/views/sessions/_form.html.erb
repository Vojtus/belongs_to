<%= form_tag sessions_path do %>

    <div>
      <%= label_tag 'name', 'Name:' %>
      <%= text_field_tag 'name' %>
    </div>

    <div>
      <%= label_tag 'password', 'Password:' %>
      <%= password_field_tag 'password' %>
    </div>
    <div>
      <%= submit_tag 'Login' %>
    </div>
<% end %>